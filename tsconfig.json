{
    "compilerOptions": {
    "outDir": "./public/",
    "noImplicitAny": true,
    "noImplicitReturns": false,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "noEmitOnError": false,
    "resolveJsonModule": true,
    "module": "es2022",
    "target": "es2022",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node"
    },
    "include": ["./src/**/*", "custom.d.ts"]
  }